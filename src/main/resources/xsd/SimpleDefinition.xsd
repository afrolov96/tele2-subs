<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	xmlns:tns="http://www.bercut.com/spec/schema/SimpleDefinition" 
	targetNamespace="http://www.bercut.com/spec/schema/SimpleDefinition" elementFormDefault="qualified" 
	version="1.5">
		
	<!-- В данном файле объявляются только простые (simpleType) типы, которые должны быть использованы во всех сложных типах данных платформы V3
	Использование стандартных xsd типов в других описаниях (в части формирования типов данных для wsdl, xsd описаний сервисов и пользовательских сервисов, и т.п. артефактов) не допускается
	Данное ограничие вводится с целью унифицировать и использовать одинаковый и законченный набор простейших типов в платформе V3, в противном случае различные подразделения смогут использовать разные типы данных с одной и той же целью, что приведет к усложнению написания бизнес-процессов и других артефактов платформы V3
	Расширение данного файла должно происходить только в том случае, если типа с таким функциональным назначением в данном файле еще нет
	Необходимо согласование расширения данного файла с архитекторами  
 -->
 
<!-- ................................................................................................................ -->
 <!-- В данном файле требуется вести простые базовые типы, которые будут использоваться в составных  типах платформы V3
 	например типы MSISDN и его ограничения, IMSI и его ограничения, цена, идентификатор валют и его возможные значения, имя и ее ограничения(+ alias'ы фамилия и отчество) и т.п.
 	чуть ниже тип DigitsId содержит комментарии, для понимания общего принципа написания простых типов и их элементов    	  
 -->
    

<!-- Простые типы и элементы платформы V3 -->
	<!-- ................................................................................................................ -->
	
<!-- ................................................................................................................ -->
	<!-- Технологические элементы / Technology elements -->
<!-- ................................................................................................................ -->    
	<xsd:element name="__unknown__" type="xsd:anyType">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Элемент для возможности расширения выбора без потери совместимости</xsd:documentation>
			<xsd:documentation xml:lang="en">Element for the possibility of extending the choice without losing compatibility</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
		
<!-- ................................................................................................................ -->	
	<!-- Цифровые идентификаторы / Digits identifiers -->
<!-- ................................................................................................................ -->
	<xsd:simpleType name="DigitsId"><!-- Объявление простого типа c именем DigitsId, используется тег simpleType и атрибут name -->
		<xsd:annotation><!-- Описание протого типа - обязательно используются теги <xsd:annotation><xsd:documentation> -->
			<xsd:documentation xml:lang="ru">Цифровой идентификатор любой сущности</xsd:documentation>
			<xsd:documentation xml:lang="en">This a digits identifier of anything</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:unsignedInt"/><!-- Ограничение данного типа, используется тег <xsd:restriction> и атрибут base в качестве значения которого должен использоватся один из стандартных простых xsd типов. Крайне желательно использование тега pattern для определения regexp строки ограничения типа (например для IMSI это было бы так <xsd:pattern value="\d{1,15}"/>) или других тегов ограничения. В случае использования перечислимого типа требуется использование тега enumeration и добавления описания в каждый вариант (в конец добавляется __unknown__ для обратной совместимости) -->		
	</xsd:simpleType>
	<xsd:element name="digitsId" type="tns:DigitsId"/><!-- Для простого использования данного типа (с помощью атрибута ref в других xsd описаниях) создается одноименный element данного типа -->
	<xsd:element name="userId" type="tns:DigitsId"><!-- Можно создать alias element данного типа с другим именем , но в этом случае обязательно добавлять свое описание -->
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Цифровой идентификатор для пользователя</xsd:documentation>
			<xsd:documentation xml:lang="en">This a digits identifier of user</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
	<xsd:element name="branchId" type="tns:DigitsId">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Цифровой идентификатор для филиала оператора</xsd:documentation>
			<xsd:documentation xml:lang="en">This a digits identifier of operators branch</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="tariffId" type="tns:DigitsId">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Цифровой идентификатор для тарифного плана оператора</xsd:documentation>
			<xsd:documentation xml:lang="en">This a digits identifier of operators tariff plan</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
	<xsd:element name="serviceId" type="tns:DigitsId">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Цифровой идентификатор для сервиса оператора</xsd:documentation>
			<xsd:documentation xml:lang="en">This a digits identifier of operators service</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="subscriberServiceId" type="tns:DigitsId">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Цифровой идентификатор для пользоваетльского сервиса оператора</xsd:documentation>
			<xsd:documentation xml:lang="en">This a digits identifier of subscriber operators service</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="productId" type="tns:DigitsId">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Цифровой идентификатор для продукта оператора</xsd:documentation>
			<xsd:documentation xml:lang="en">This a digits identifier of operators product</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="billingId" type="tns:DigitsId">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Цифровой идентификатор для экземпляра биллинга оператора(в случае Беркут - In@Voice DB)</xsd:documentation>
			<xsd:documentation xml:lang="en">This a digits identifier of instance of operators billing system</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="objectId" type="tns:DigitsId">
         <xsd:annotation>
             <xsd:documentation xml:lang="ru">Цифровой идентификатор объекта или профиля</xsd:documentation>
             <xsd:documentation xml:lang="en">This a digits identifier of object or profile</xsd:documentation>
         </xsd:annotation>
     </xsd:element>
	<xsd:element name="clientId" type="tns:DigitsId">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Цифровой идентификатор для клиента (в случае Беркут - клиент In@Voice DB)</xsd:documentation>
			<xsd:documentation xml:lang="en">This a digits identifier of instance of operators billing system</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
     
     <xsd:element name="jobId" type="tns:DigitsId">
         <xsd:annotation>
             <xsd:documentation xml:lang="ru">Цифровой идентификатор задачи</xsd:documentation>
             <xsd:documentation xml:lang="en">This a digits identifier of job</xsd:documentation>
         </xsd:annotation>
     </xsd:element>
		
<!-- ................................................................................................................ -->
	<!-- Строковые идентификаторы / String identifiers -->
<!-- ................................................................................................................ -->
	<xsd:simpleType name="UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Строковый идентификатор любой сущности</xsd:documentation>
			<xsd:documentation xml:lang="en">This a string identifier of anything</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value=".{1,64}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="uniqueName" type="tns:UniqueName"/>
	<xsd:element name="name" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Имя какой-либо сущности</xsd:documentation>
			<xsd:documentation xml:lang="en">Name of anything</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
    <xsd:element name="correlationId" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Строковый корреляционный идентификатор</xsd:documentation>
			<xsd:documentation xml:lang="en">String correlation id</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="typeName" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Название типа какой-либо сущности</xsd:documentation>
			<xsd:documentation xml:lang="en">Name of type of anything</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
	<xsd:element name="userName" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Имя пользователя</xsd:documentation>
			<xsd:documentation xml:lang="en">User name</xsd:documentation>
		</xsd:annotation>
	</xsd:element>		
    <xsd:element name="serviceName" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Имя сервиса</xsd:documentation>
			<xsd:documentation xml:lang="en">Service name</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
    <xsd:element name="systemName" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Имя системы</xsd:documentation>
			<xsd:documentation xml:lang="en">System name</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
    <xsd:element name="groupName" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Имя группы</xsd:documentation>
			<xsd:documentation xml:lang="en">Group name</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
    <xsd:element name="roleName" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Имя роли</xsd:documentation>
			<xsd:documentation xml:lang="en">Role name</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
    <xsd:element name="applicationName" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Название приложения</xsd:documentation>
			<xsd:documentation xml:lang="en">Application name</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
    <xsd:element name="productTypeName" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Наименование типа продукта</xsd:documentation>
			<xsd:documentation xml:lang="en">Name of product type</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
    <xsd:element name="productName" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Наименование продукта</xsd:documentation>
			<xsd:documentation xml:lang="en">Product name</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="operatorName" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Наименование оператора</xsd:documentation>
			<xsd:documentation xml:lang="en">Operator name</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="countryName" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Наименование страны</xsd:documentation>
			<xsd:documentation xml:lang="en">Country name</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
    <xsd:element name="regionName" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Наименование региона</xsd:documentation>
			<xsd:documentation xml:lang="en">Region name</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
	<xsd:element name="settlementName" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Наименование поселения</xsd:documentation>
			<xsd:documentation xml:lang="en">Settlement name</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
	<xsd:element name="operatorOwnerId" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Мнемоника оператора</xsd:documentation>
			<xsd:documentation xml:lang="en">Operator mnemo</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="defOwnerId" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Мнемоника владельца номерной ёмкости</xsd:documentation>
			<xsd:documentation xml:lang="en">MSISDN DEF owner mnemo</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="schedulerName" type="tns:UniqueName">
         <xsd:annotation>
             <xsd:documentation xml:lang="ru">Наименование шедулера</xsd:documentation>
             <xsd:documentation xml:lang="en">Name of scheduler</xsd:documentation>
         </xsd:annotation>
     </xsd:element>

    <xsd:element name="serviceTypeName" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Тип сервиса</xsd:documentation>
			<xsd:documentation xml:lang="en">Service type</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
    <xsd:element name="resourceTypeName" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Тип ресурса</xsd:documentation>
			<xsd:documentation xml:lang="en">Resource type</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

    <xsd:element name="tag" type="tns:UniqueName" default="create">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Идентификатор для категоризации, поиска данных и задания временных меток</xsd:documentation>
			<xsd:documentation xml:lang="en">Identifier for the categorization and retrieval data by timestamp</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:simpleType name="GUID">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Cтатистически уникальный 128-битный идентификатор</xsd:documentation>
			<xsd:documentation xml:lang="en">Globally Unique Identifier is a unique reference number used as an identifier</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9A-F]{8}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{12}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="guid" type="tns:GUID"/>	
	<xsd:element name="sessionId" type="tns:GUID">
			<xsd:annotation>
			<xsd:documentation xml:lang="ru">Cтатистически уникальный 128-битный идентификатор сессии</xsd:documentation>
			<xsd:documentation xml:lang="en">Globally Unique Identifier is a unique reference number used as session identifier</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
    <xsd:element name="resourceId" type="tns:GUID">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">Уникальный 128-битный идентификатор ресурса, например, отчета, медиа-контента </xsd:documentation>
            <xsd:documentation xml:lang="en">Unique 128-bit Identifier of resource(report, media file, etc.)</xsd:documentation>
        </xsd:annotation>
    </xsd:element>	
	
<!-- ................................................................................................................ -->
	<!-- Интернет идентификаторы / Internet identifiers -->
<!-- ................................................................................................................ -->
	<xsd:element name="login" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Логин пользователя</xsd:documentation>
			<xsd:documentation xml:lang="en">User login</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	

	<xsd:simpleType name="Password">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Пароль пользователя в явном виде (не хешированый), может содержать от 4 до 64 символов</xsd:documentation>
			<xsd:documentation xml:lang="en">A password is a secret word or string of characters used for user authentication to prove identity or access approval to gain access to a resource access</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value=".{4,64}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="password" type="tns:Password"/>	
	
	<xsd:simpleType name="EMail">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Адрес электронной почты, требования к значению http://tools.ietf.org/html/rfc822</xsd:documentation>
			<xsd:documentation xml:lang="en">An email address identifies an email box to which email messages are delivered</xsd:documentation>
		</xsd:annotation>			
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="^\S+@\S+\.\S+$"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="email" type="tns:EMail"/>

	<xsd:simpleType name="URI">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Универсальный идентификатор ресурса</xsd:documentation>
			<xsd:documentation xml:lang="en">Uniform resource identifier</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:anyURI" />
	</xsd:simpleType>

	<xsd:element name="url" type="tns:URI">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Универсальный указатель на ресурс</xsd:documentation>
			<xsd:documentation xml:lang="en">Uniform resource locator</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="namespace" type="tns:URI">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Универсальный указатель на пространство имён</xsd:documentation>
			<xsd:documentation xml:lang="en">Uniform namespace locator</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
<!-- ................................................................................................................ -->
	<!-- Мобильный идентификаторы / Mobile identifiers -->
<!-- ................................................................................................................ -->
	<xsd:simpleType name="MSISDN">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Номер мобильного абонента цифровой сети с интеграцией служб для связи (MSISDN - Mobile Subscriber Integrated Services Digital Number) в стандартах GSM, UMTS и пр.</xsd:documentation>
			<xsd:documentation xml:lang="en">Mobile Subscriber Integrated Services Digital Number (MSISDN) is a number uniquely identifying a subscription in a GSM or a UMTS mobile network</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(\d|[A-D]|\*|#){1,15}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="msisdn" type="tns:MSISDN"/>
	<xsd:element name="pstnNumber" type="tns:MSISDN">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Номер абонента в телефонной сети общего пользоватия (PSTN - Public Switched Telephone Network)</xsd:documentation>
			<xsd:documentation xml:lang="en">Phone number in Public Switched Telephone Network (PSTN)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="vlr" type="tns:MSISDN">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Номер временной базы хранения абонентов (VLR - Visitor Location Register) в стандартах GSM, UMTS и пр</xsd:documentation>
			<xsd:documentation xml:lang="en">Number of Visitor Location Register (VLR) in a GSM or a UMTS mobile network</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="unstructuredPhoneNumber" type="tns:UniqueName">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Телефонный номер в произвольном формате</xsd:documentation>
			<xsd:documentation xml:lang="en">Unstructed/unspecified phone number</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	
	<xsd:simpleType name="IMSI">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Международный идентификатор мобильного абонента (индивидуальный номер абонента), ассоциированный с каждым пользователем мобильной связи стандарта GSM, UMTS или CDMA</xsd:documentation>
			<xsd:documentation xml:lang="en">International Mobile Subscriber Identity is used to identify the user of a cellular network and is a unique identification associated with all cellular networks</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{1,15}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="imsi" type="tns:IMSI"/>	

	<xsd:simpleType name="RusRoutingNumber">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Номер для маршрутизации в России, записанный одной строкой формат записи: D|GeoCode|MNC</xsd:documentation>
			<xsd:documentation xml:lang="en">A routing number in Russia in format D|GeoCode|MNC</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="D\d{4,4}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="rusRoutingNumber" type="tns:RusRoutingNumber"/>

<!-- ................................................................................................................ -->
	<!-- Коды / Codes -->
<!-- ................................................................................................................ -->
	<xsd:simpleType name="Code">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Абстрактный код чего-либо (не обязательно является числом)</xsd:documentation>
			<xsd:documentation xml:lang="en">A abstract code of anything</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:element name="code" type="tns:Code"/>

	<xsd:simpleType name="ThreeDigitCode">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Абстрактный трех значный код чего-либо (не является числом)</xsd:documentation>
			<xsd:documentation xml:lang="en">A abstract three digit code</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="threeDigitCode" type="tns:ThreeDigitCode"/>
	<xsd:element name="mcc" type="tns:ThreeDigitCode">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Мобильный код страны в комбинации с MNC является уникальным идентификатором сотового оператора сетей GSM, CDMA, iDEN, TETRA и UMTS, а также некоторых операторов спутниковой связи</xsd:documentation>
			<xsd:documentation xml:lang="en">A Mobile Country Code is used in combination with a mobile network code (MCC) (also known as a "MCC / MNC tuple") to uniquely identify a mobile phone operator/carrier using the GSM/LTE, CDMA, iDEN, TETRA and UMTS public land mobile networks and some satellite mobile networks</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:simpleType name="TwoDigitCode">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Абстрактный двух значный код чего-либо (не является числом)</xsd:documentation>
			<xsd:documentation xml:lang="en">A abstract two digit code</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="twoDigitCode" type="tns:TwoDigitCode"/>
	<xsd:element name="mnc" type="tns:TwoDigitCode">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Код мобильной сети в комбинации с MCC является уникальным идентификатором сотового оператора сетей GSM, CDMA, iDEN, TETRA и UMTS, а также некоторых операторов спутниковой связи</xsd:documentation>
			<xsd:documentation xml:lang="en">A Mobile Network Code is used in combination with a mobile country code (MCC) (also known as a "MCC / MNC tuple") to uniquely identify a mobile phone operator/carrier using the GSM/LTE, CDMA, iDEN, TETRA and UMTS public land mobile networks and some satellite mobile networks</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="geoCode" type="tns:TwoDigitCode">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Код географического региона</xsd:documentation>
			<xsd:documentation xml:lang="en">Code of geographical entity</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:simpleType name="DCS">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Схема кодирования данных для SMS</xsd:documentation>
			<xsd:documentation xml:lang="en">Data Coding Schema for SMS</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:byte"/>
	</xsd:simpleType>	
	<xsd:element name="dcs" type="tns:DCS"/>


<!-- ................................................................................................................ -->
	<!-- Текстовые элементы / Text elements -->
<!-- ................................................................................................................ -->
	<xsd:simpleType name="Text">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Абстрактный текст не ограниченной длины</xsd:documentation>
			<xsd:documentation xml:lang="en">Abstract text</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:element name="text" type="tns:Text"/>
	<xsd:element name="description" type="tns:Text">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Описание любой сущности</xsd:documentation>
			<xsd:documentation xml:lang="en">Description text of anything.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="smsTextUnboundedSegments" type="tns:Text">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">SMS текст c неограниченным количеством сегментов</xsd:documentation>
			<xsd:documentation xml:lang="en">SMS text with unbounded number of segments</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
