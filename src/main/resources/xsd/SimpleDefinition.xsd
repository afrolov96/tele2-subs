<!-- ................................................................................................................ -->
	<!-- Количественные элементы / Quantitative elements -->
<!-- ................................................................................................................ -->
	<xsd:simpleType name="Number">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Количество чего-либо</xsd:documentation>
			<xsd:documentation xml:lang="en">This a number of anything</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:unsignedInt"/>
	</xsd:simpleType>
	<xsd:element name="number" type="tns:Number"/>
	<xsd:element name="dayNumber" type="tns:Number">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Количество дней</xsd:documentation>
			<xsd:documentation xml:lang="en">This a number of day</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="pageNumber" type="tns:Number">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Номер страницы</xsd:documentation>
			<xsd:documentation xml:lang="en">This a page number</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="recordPerPage" type="tns:Number">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Количество записей на странице</xsd:documentation>
			<xsd:documentation xml:lang="en">Number of record per page</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
				
	<xsd:simpleType name="Percentage">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Проценты чего-либо</xsd:documentation>
			<xsd:documentation xml:lang="en">This a percentage of anything</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:unsignedInt">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="100"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="percentage" type="tns:Percentage"/>
	<xsd:element name="commissionPercentage" type="tns:Percentage">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Процент комиссии</xsd:documentation>
			<xsd:documentation xml:lang="en">This a percentage of commision</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:simpleType name="MoneyAmount">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Количество денег не привязаное к конкретной валюте</xsd:documentation>
			<xsd:documentation xml:lang="en">This is amount of money (for any currency)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:fractionDigits value="6"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="moneyAmount" type="tns:MoneyAmount"/>
	<xsd:element name="rubMoneyAmount" type="tns:MoneyAmount">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Количество рублей</xsd:documentation>
			<xsd:documentation xml:lang="en">This is amount of russian money</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

<!-- ................................................................................................................ -->
	<!-- Временные элементы / Time elements -->
<!-- ................................................................................................................ -->

	<xsd:simpleType name="DateTimeStamp">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Временная метка (дата и время)</xsd:documentation>
			<xsd:documentation xml:lang="en">Time stamp (date and time)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:dateTime"/>
	</xsd:simpleType>
	<xsd:element name="dateTimeStamp" type="tns:DateTimeStamp"/>
	<xsd:element name="startDateTime" type="tns:DateTimeStamp">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Дата и время начала события</xsd:documentation>
			<xsd:documentation xml:lang="en">Date and time of the begin event</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
	<xsd:element name="endDateTime" type="tns:DateTimeStamp">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Дата и время окончания события</xsd:documentation>
			<xsd:documentation xml:lang="en">Date and time of the end event</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:simpleType name="TimeStamp">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Временная метка (только время)</xsd:documentation>
			<xsd:documentation xml:lang="en">Time stamp (only time)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:dateTime"/>
	</xsd:simpleType>
	<xsd:element name="timeStamp" type="tns:TimeStamp"/>
	<xsd:element name="startTime" type="tns:TimeStamp">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Время начала события</xsd:documentation>
			<xsd:documentation xml:lang="en">Time of the begin event</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
	<xsd:element name="endTime" type="tns:TimeStamp">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Время окончания события</xsd:documentation>
			<xsd:documentation xml:lang="en">Time of the end event</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:simpleType name="Delay">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Временная задержка</xsd:documentation>
			<xsd:documentation xml:lang="en">Тemporary delay</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:duration"/>
	</xsd:simpleType>
	<xsd:element name="delay" type="tns:Delay"/>
	<xsd:element name="duration" type="tns:Delay">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Длительность</xsd:documentation>
			<xsd:documentation xml:lang="en">Duration</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="period" type="tns:Delay">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Период между событиями</xsd:documentation>
			<xsd:documentation xml:lang="en">Period beetwin events</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="timeZone" type="tns:Delay">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Сдвиг времени для часового пояса относительно времени Гринвича</xsd:documentation>
			<xsd:documentation xml:lang="en">Time shift for the time zone relative to Greenwich time</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
		
	<xsd:simpleType name="Minute">
         <xsd:annotation>
             <xsd:documentation xml:lang="ru">Значение времени - минуты</xsd:documentation>
             <xsd:documentation xml:lang="en">Value of time - minutes</xsd:documentation>
         </xsd:annotation>
         <xsd:restriction base="xsd:unsignedByte">
             <xsd:minInclusive value="0"></xsd:minInclusive>
             <xsd:maxInclusive value="59"></xsd:maxInclusive>
         </xsd:restriction>
     </xsd:simpleType>
     <xsd:element name="minute" type="tns:Minute"/>
     
     <xsd:simpleType name="Hour">
         <xsd:annotation>
             <xsd:documentation xml:lang="ru">Значение времени - часы</xsd:documentation>
             <xsd:documentation xml:lang="en">Value of time - hours</xsd:documentation>
         </xsd:annotation>
         <xsd:restriction base="xsd:unsignedByte">
             <xsd:minInclusive value="0"></xsd:minInclusive>
             <xsd:maxInclusive value="23"></xsd:maxInclusive>
         </xsd:restriction>
     </xsd:simpleType>
     <xsd:element name="hour" type="tns:Hour"/>

     <xsd:simpleType name="DayOfMonth">
         <xsd:annotation>
             <xsd:documentation xml:lang="ru">Значение времени - день месяца</xsd:documentation>
             <xsd:documentation xml:lang="en">Value of time - day of month type</xsd:documentation>
         </xsd:annotation>
         <xsd:restriction base="xsd:unsignedByte">
             <xsd:minInclusive value="1"></xsd:minInclusive>
             <xsd:maxInclusive value="31"></xsd:maxInclusive>
         </xsd:restriction>
     </xsd:simpleType>
     <xsd:element name="dayOfMonth" type="tns:DayOfMonth"/>

	<xsd:simpleType name="DayOfWeek">
 		<xsd:annotation>
 			<xsd:documentation xml:lang="ru">День недели (понедельник, вторник, среда...)</xsd:documentation>
 			<xsd:documentation xml:lang="en">Name of day (Monday, Tuesday, Wednesday...)</xsd:documentation>
 		</xsd:annotation>
 		<xsd:restriction base="xsd:string">
 			<xsd:enumeration value="Monday"/>
 			<xsd:enumeration value="Tuesday"/>
             <xsd:enumeration value="Wednesday"/>
             <xsd:enumeration value="Thursday"/>
             <xsd:enumeration value="Friday"/>
             <xsd:enumeration value="Saturday"/>
             <xsd:enumeration value="Sunday"/>
 		</xsd:restriction>
 	</xsd:simpleType>
 	<xsd:element name="dayOfWeek" type="tns:DayOfWeek"/>	

     <xsd:simpleType name="Month">
         <xsd:annotation>
             <xsd:documentation xml:lang="ru">Значение времени -  месяц (январь, февраль, март...)</xsd:documentation>
             <xsd:documentation xml:lang="en">Value of time - month</xsd:documentation>
         </xsd:annotation>
 		 <xsd:restriction base="xsd:string">			 
 			 <xsd:enumeration value="January"/>
 			 <xsd:enumeration value="February"/>
 			 <xsd:enumeration value="March"/>
 			 <xsd:enumeration value="April"/>
 			 <xsd:enumeration value="May"/>
 			 <xsd:enumeration value="June"/>
 			 <xsd:enumeration value="July"/>
 			 <xsd:enumeration value="August"/>
 			 <xsd:enumeration value="September"/>
 			 <xsd:enumeration value="October"/>
 			 <xsd:enumeration value="November"/>
 			 <xsd:enumeration value="December"/>	
 		 </xsd:restriction>
 	 </xsd:simpleType>
	 <xsd:element name="month" type="tns:Month"/>

<!-- ................................................................................................................ -->
	<!-- Логические элементы / Logic elements -->
<!-- ................................................................................................................ -->

	
	<xsd:simpleType name="YesNo">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Абстрактный параметр только два значения да или нет</xsd:documentation>
			<xsd:documentation xml:lang="en">Abstract parameter only yes or no value</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:boolean"/>
	</xsd:simpleType>
	<xsd:element name="yesNo" type="tns:YesNo"/>
	<xsd:element name="availability" type="tns:YesNo">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Доступность чего-либо, только да или нет</xsd:documentation>
			<xsd:documentation xml:lang="en">This a availability - true or false</xsd:documentation>
		</xsd:annotation>
	</xsd:element>	
	<xsd:element name="switchOnOff" type="tns:YesNo">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Двух позиционный переключатель - включен или выключен</xsd:documentation>
			<xsd:documentation xml:lang="en">Two position switch - on/off</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="fraudSuspicion" type="tns:YesNo">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Подозрение в мошенничестве, только да или нет</xsd:documentation>
			<xsd:documentation xml:lang="en">Fraud suspicion  - true or false</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	    
	
	
	<xsd:simpleType name="RegExp">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Содержит любюе регулярное выражение</xsd:documentation>
			<xsd:documentation xml:lang="en">Uniform regexp expression</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string" /> <!-- TO DO  -->
	</xsd:simpleType>
	<xsd:element name="regexp" type="tns:RegExp"/>
	<xsd:element name="mask" type="tns:RegExp">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Маска для любой строки</xsd:documentation>
			<xsd:documentation xml:lang="en">Mask for any string</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="msisdnMask" type="tns:RegExp">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Маска(регулярное выражение) для MSISDN</xsd:documentation>
			<xsd:documentation xml:lang="en">Mask (regular expression) for MSISDN</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="vlrMask" type="tns:RegExp">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Маска(регулярное выражение) для VLR</xsd:documentation>
			<xsd:documentation xml:lang="en">Mask (regular expression) for VLR</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
    <xsd:simpleType name="XPath">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Содержит x-path выражение</xsd:documentation>
			<xsd:documentation xml:lang="en">X-path expression</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:element name="xpath" type="tns:XPath" default="/"/>

<!-- ................................................................................................................ -->
	<!-- Перечислимые элементы / Enumeration elements -->
<!-- ................................................................................................................ -->
	<xsd:simpleType name="SubscriberChargingType">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Тип тарификации абонента - в процессе звонка или после звонка</xsd:documentation>
			<xsd:documentation xml:lang="en">This a restriction base type of charging for operators subscribers: online(prepaid) and offline(postpaid)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="prepaid">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">тарификация абонента в процессе звонка</xsd:documentation>
					<xsd:documentation xml:lang="en">online charging for subscriber</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="postpaid">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">тарификация абонента после звонка</xsd:documentation>
					<xsd:documentation xml:lang="en">offline charging for subscriber</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="__unknown__">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">неизвестное значение (для возможности расширения набора без потери совместимости)</xsd:documentation>
					<xsd:documentation xml:lang="en">unknown value (for the possibility of extending the set without losing compatibility)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="subscriberChargingType" type="tns:SubscriberChargingType" default="prepaid"/>	
	
	<xsd:simpleType name="FaultCategory">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Категория исключительного события</xsd:documentation>
			<xsd:documentation xml:lang="en">Category of fault event</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="fatal">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">Событие, после которого не возможно корректно продолжить работу</xsd:documentation>
					<xsd:documentation xml:lang="en">Event, after which it is impossible to continue to work correctly</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="error">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">Произошла ошибка, после которой возможно продолжение работы</xsd:documentation>
					<xsd:documentation xml:lang="en">An error has occurred, after which possible continue of work</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="notice">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">Произошло событие, на которое стоит обратить внимание</xsd:documentation>
					<xsd:documentation xml:lang="en">There was an event to which you should pay attention</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="__unknown__">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">неизвестное значение (для возможности расширения набора без потери совместимости)</xsd:documentation>
					<xsd:documentation xml:lang="en">unknown value (for the possibility of extending the set without losing compatibility)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>			
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="faultCategory" type="tns:FaultCategory" default="error"/>

	<xsd:simpleType name="ServiceStatus">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Cодержит набор возможных статусов для сервисов</xsd:documentation>
			<xsd:documentation xml:lang="en">Set of availible service status</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="active">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">Сервис активен</xsd:documentation>
					<xsd:documentation xml:lang="en">Аctive service</xsd:documentation>		
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="unactive">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">Сервис неактивен</xsd:documentation>
					<xsd:documentation xml:lang="en">Non active service</xsd:documentation>		
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="blocked">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">Сервис заблокирован</xsd:documentation>
					<xsd:documentation xml:lang="en">Service blocked</xsd:documentation>		
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="unblocked">			
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">Сервис разблокирован</xsd:documentation>
					<xsd:documentation xml:lang="en">Service unblocked</xsd:documentation>		
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="testing">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">Сервис тестируется</xsd:documentation>
					<xsd:documentation xml:lang="en">Service testing</xsd:documentation>		
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="archived">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">Сервис в архиве</xsd:documentation>
					<xsd:documentation xml:lang="en">Service archived</xsd:documentation>		
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="canceled">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">Сервис отменен</xsd:documentation>
					<xsd:documentation xml:lang="en">Service canceled</xsd:documentation>		
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="__unknown__">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">неизвестное значение (для возможности расширения набора без потери совместимости)</xsd:documentation>
					<xsd:documentation xml:lang="en">unknown value (for the possibility of extending the set without losing compatibility)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>			
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="serviceStatus" type="tns:ServiceStatus" default="unactive"/>

    <xsd:simpleType name="ResourceStatus">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">Cодержит набор возможных статусов для ресурсов</xsd:documentation>
			<xsd:documentation xml:lang="en">Set of availible service status</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="active">
				<xsd:annotation>
					<xsd:documentation xml:lang="ru">Ресурс доступен</xsd:documentation>
					<xsd:documentation xml:lang="en">Аctive resource</xsd:documentation>		
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="deleted">
